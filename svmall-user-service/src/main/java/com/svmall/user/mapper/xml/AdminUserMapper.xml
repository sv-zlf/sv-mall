<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.svmall.user.mapper.AdminUserMapper">

    <select id="login" parameterType="String" resultType="com.svmall.user.entity.AdminUser">
        select * from admin_user where user_name=#{userName} and password=#{password};
    </select>

</mapper>
